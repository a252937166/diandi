@import "../../components/diaryCard/diaryCard.wxss";

.container {
  padding: 5px 15px 0px 15px;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.add-nav {
  position: fixed;
  z-index: 999;
  bottom: 62px;
  right: 14px;
  display: flex;
  height: 48px;
  width: 48px;
  flex-direction: column;
  border-radius: 24px;
  background-color:rgba(255,255,255,0.6);
  align-items: center;
  box-shadow: 0 0px 10px #ff9900;
  justify-content: center;
}
.add-image{
  height: 30px;
  width: 30px;
}